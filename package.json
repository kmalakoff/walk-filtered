{
  "name": "walk-filtered",
  "version": "2.6.22",
  "description": "A simple, performant file system walker to provided fine-grained control over directories and files to walk. Supports Node 0.10 and above.",
  "keywords": [
    "recursive",
    "fs",
    "readdir",
    "filesystem",
    "find",
    "filter",
    "walk",
    "walk-filtered"
  ],
  "homepage": "https://github.com/kmalakoff/walk-filtered",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/walk-filtered.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "main": "dist/cjs/index.cjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "fs-iterator": "^5.2.21",
    "fs-stats-spys": "^1.1.14"
  },
  "devDependencies": {
    "@types/mocha": "*",
    "@types/node": "*",
    "fs-generate": "*",
    "pinkie-promise": "*",
    "rimraf2": "*",
    "starts-with": "*"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.mjs",
    "targets": [
      "cjs"
    ]
  }
}
