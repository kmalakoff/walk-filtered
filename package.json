{
  "name": "walk-filtered",
  "version": "2.6.0",
  "description": "A simple, performant file system walker to provided fine-grained control over directories and files to walk. Supports Node 0.10 and above.",
  "keywords": [
    "recursive",
    "fs",
    "readdir",
    "filesystem",
    "find",
    "filter",
    "walk",
    "walk-filtered"
  ],
  "homepage": "https://github.com/kmalakoff/walk-filtered",
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/walk-filtered.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "deploy": "tsds deploy",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "fs-iterator": "^5.2.0",
    "fs-stats-spys": "^1.1.0",
    "pinkie-promise": "^2.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "depcheck": "^1.4.7",
    "fs-generate": "^1.8.8",
    "rimraf2": "^2.8.2",
    "starts-with": "^1.0.2",
    "ts-dev-stack": "^1.5.4"
  },
  "packageManager": "npm@11.0.0+sha512.11dff29565d2297c74e7c594a9762581bde969f0aa5cbe6f5b3644bf008a16c065ece61094d9ffbb81125be38df8e1ba43eb8244b3d30c61eb797e9a2440e3ec",
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.mjs",
    "targets": [
      "cjs"
    ]
  }
}
