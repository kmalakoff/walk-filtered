{
  "name": "walk-filtered",
  "version": "1.9.0",
  "description": "A simple, performant file system walker to provided fine-grained control over directories and files to walk. Supports Node 0.10 and above.",
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha . test/spec/**/*.test.js",
    "perf": "node test/perf/tests.js",
    "lint": "eslint .",
    "format": "prettier --write '**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/walk-filtered.git"
  },
  "keywords": [
    "recursive",
    "fs",
    "readdir",
    "filesystem",
    "find",
    "filter",
    "walk",
    "walk-filtered"
  ],
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "license": "MIT",
  "homepage": "https://github.com/kmalakoff/walk-filtered",
  "dependencies": {
    "queue-cb": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "benchmark": "^2.1.4",
    "chai": "^4.1.2",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "fs-generate": "^1.1.0",
    "graceful-fs": "^4.1.15",
    "mocha": "^5.2.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.2",
    "sinon": "^7.2.3",
    "sinon-chai": "^3.3.0"
  }
}
